{
  "extends": ["airbnb-base", "plugin:react/recommended"],
  "env": {
    "jest": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "react/display-name" : 0,
    "eol-last": 0,
    "semi":0,
    "no-console":0,
    "no-prototype-builtins": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "import/no-cycle": 0,
    "iterators/generators": 0,
    "import/prefer-default-export": 0
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 10,
    "esversion": 7
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
